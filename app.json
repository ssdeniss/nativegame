{
  "expo": {
    "name": "worms",
    "slug": "wormsapp",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.wormix.worm"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "rnpm": {
      "assets": [
        "./src/assets/fonts"
      ]
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "extra": {
      "eas": {
        "projectId": "8e54ca98-2743-485a-82ca-5abbfb0f988c"
      }
    }
  }
}
